= form_for(@dimention) do |f|
  .field
    = f.label :title
    %br
      = f.text_field :title
  .field
    = f.label :weight
    %br
      = f.number_field :weight
  .form_row
    = f.label :priority_id, "Prioridad"
    %br
      = f.collection_select :priority_id, Priority.order(:weight), :id, :title, {}, class: "form_select"
  .actions
    = f.submit
